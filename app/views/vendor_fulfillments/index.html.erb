    <!-- Header -->
    <div class="header bg-primary pb-6">
      <div class="container-fluid">
        <div class="header-body">
          <div class="row align-items-center py-4">
            <div class="col-lg-6 col-7">
              <h6 class="h2 text-white d-inline-block mb-0">Fulfillment</h6>
            </div>
            <div class="col-lg-6 col-5 text-right">
              <%= link_to 'New', new_vendor_order_vendor_fulfillment_path(@order), class: 'btn btn-sm btn-neutral' %>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- Page content -->
    <div class="container-fluid mt--6">
      <div class="row">
        <div class="col">
          <div class="card">
            <!-- Card header -->
            <div class="card-header border-0">
              <h3 class="mb-0">Fulfillment</h3>
            </div>
            <!-- Light table -->
            <div class="table-responsive">
              <table class="table align-items-center table-flush">
                <thead class="thead-light">
                  <tr>
                    <th scope="col" class="sort" data-sort="name">Order ID</th>
                    <th scope="col" class="sort" data-sort="budget">Product</th>
                    <th scope="col" class="sort" data-sort="budget">Variant</th>
                    <th scope="col" class="sort" data-sort="budget">Tracking number</th>
                    <th scope="col" class="sort" data-sort="budget">Tracking URL</th>
                    <th scope="col" class="sort" data-sort="budget">Quantity</th>
                    <th scope="col" class="sort" data-sort="status">Status</th>
                    <th scope="col" class="sort" data-sort="completion">Date</th>
                    <th scope="col"></th>
                  </tr>
                </thead>
                <tbody class="list">
                  <% @vendor_fulfillments.each do |fulfillment|%>
                    <tr>
                      <th scope="row">
                        <div class="media align-items-center">
                          <div class="media-body">
                            <span class="name mb-0 text-sm"><%= fulfillment.vendor_order.shopify_order_data['name']%></span>
                          </div>
                        </div>
                      </th>
                      <td class="budget">
                        <%= fulfillment.vendor_order.vendor_product.shopify_product_data['title']%>
                      </td>
                      <td class="budget">
                        <%= fulfillment.vendor_order.vendor_variant.shopify_variant_data['title']%>
                      </td>
                      <td class="budget">
                        <%= fulfillment.shopify_fulfillment_data['tracking_number']%>
                      </td>
                      <td class="budget">
                        <%= fulfillment.shopify_fulfillment_data['tracking_url']%>
                      </td>
                      <td class="budget">
                        <%= fulfillment.shopify_fulfillment_count%>
                      </td>
                      <td class="budget">
                        <%= fulfillment.shopify_fulfillment_status%>
                      </td>
                      <td>
                        <%= fulfillment.shopify_fulfillment_data['created_at']%>
                      </td>
                    </tr>
                  <% end %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>